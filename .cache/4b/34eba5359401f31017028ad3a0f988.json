4}},{"generated":{"line":65,"column":38},"source":"projects/evens-or-odds/components/Card.js","original":{"line":67,"column":33}}],"sources":{"projects/evens-or-odds/components/Card.js":"import React from 'react';\r\nimport { connect } from 'react-redux';\r\nimport { playerCorrect, playerWrong } from '../actions/player';\r\n\r\nimport cardBack from '../assets/cardBack.png';\r\n\r\nconst Card = props => {\r\n    console.log(props)\r\n\r\n    const {currentCard, guess} = props;\r\n    var curcard;\r\n\r\n    if(currentCard == null){\r\n        curcard = (\r\n            <div>\r\n                <h3>Start guessing to start the game!</h3>\r\n                <img src={cardBack} alt=\"card image\" style={{height:'400px'}}/>\r\n            </div>\r\n        )\r\n    } else {\r\n        if ((guess == \"even\" && \r\n            (currentCard.value % 2 == 0 || \r\n                currentCard.value == \"KING\" || \r\n                currentCard.value == \"QUEEN\" || \r\n                currentCard.value == \"JACK\" )) || (guess == \"odd\" && currentCard.value % 2 == 1))\r\n        {\r\n            props.playerCorrect();\r\n        } else {\r\n            props.playerWrong();\r\n        }\r\n        curcard = (\r\n            <div>\r\n                <h3>{currentCard.value} of {currentCard.suit}</h3>\r\n                <img src={currentCard.images.png} alt=\"card image\" style={{height:'400px'}}/>\r\n            </div>\r\n        )\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            {curcard}\r\n            <hr></hr>\r\n        </div>\r\n    );\r\n}\r\n\r\nconst mapStateToProps = state => {\r\n    const {\r\n        deck : { currentCard },\r\n        player: { guess }\r\n    } = state;\r\n\r\n    return {\r\n        currentCard,\r\n        guess\r\n    };\r\n}\r\n\r\nconst componentConnector = connect(\r\n    mapStateToProps,\r\n    {\r\n        playerCorrect,\r\n        playerWrong\r\n    }\r\n);\r\n\r\nexport default componentConnector(Card);"},"lineCount":null}},"error":null,"hash":"038fd4214e8397760a793c82d6a87cb0","cacheData":{"env":{}}}